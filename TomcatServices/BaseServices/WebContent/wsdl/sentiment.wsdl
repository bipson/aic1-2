<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="SentimentService" targetNamespace="http://aic.service.sentiment/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="http://aic.service.sentiment/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">
  <wsdl:types>
    <xs:schema elementFormDefault="unqualified" targetNamespace="http://aic.service.sentiment/" version="1.0" xmlns:tns="http://aic.service.sentiment/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
<xs:element name="Compute" type="tns:Compute"/>
<xs:element name="ComputeAverage" type="tns:ComputeAverage"/>
<xs:element name="ComputeAverageResponse" type="tns:ComputeAverageResponse"/>
<xs:element name="ComputeResponse" type="tns:ComputeResponse"/>
<xs:complexType name="ComputeAverage">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="texts" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
<xs:complexType name="ComputeAverageResponse">
    <xs:sequence>
      <xs:element minOccurs="0" name="averageSentiment" type="xs:double"/>
    </xs:sequence>
  </xs:complexType>
<xs:complexType name="Compute">
    <xs:sequence>
      <xs:element minOccurs="0" name="text" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
<xs:complexType name="ComputeResponse">
    <xs:sequence>
      <xs:element minOccurs="0" name="sentiment" type="xs:double"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>
  </wsdl:types>
  <wsdl:message name="ComputeResponse">
    <wsdl:part name="parameters" element="tns:ComputeResponse">
    </wsdl:part>
  </wsdl:message>
  <wsdl:message name="ComputeAverageResponse">
    <wsdl:part name="parameters" element="tns:ComputeAverageResponse">
    </wsdl:part>
  </wsdl:message>
  <wsdl:message name="Compute">
    <wsdl:part name="parameters" element="tns:Compute">
    </wsdl:part>
  </wsdl:message>
  <wsdl:message name="ComputeAverage">
    <wsdl:part name="parameters" element="tns:ComputeAverage">
    </wsdl:part>
  </wsdl:message>
  <wsdl:portType name="ISentiment">
    <wsdl:operation name="ComputeAverage">
      <wsdl:input name="ComputeAverage" message="tns:ComputeAverage">
    </wsdl:input>
      <wsdl:output name="ComputeAverageResponse" message="tns:ComputeAverageResponse">
    </wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="Compute">
