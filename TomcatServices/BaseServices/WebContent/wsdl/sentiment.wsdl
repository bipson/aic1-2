    </wsdl:output>
    </wsdl:operation>
  </wsdl:portType>
  <wsdl:binding name="SentimentServiceSoapBinding" type="tns:ISentiment">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="Compute">
      <soap:operation soapAction="urn:Compute" style="document"/>
      <wsdl:input name="Compute">
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output name="ComputeResponse">
        <soap:body use="literal"/>
      </wsdl:output>
