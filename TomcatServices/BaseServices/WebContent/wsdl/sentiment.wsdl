        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output name="ComputeAverageResponse">
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="SentimentService">
    <wsdl:port name="Sentiment" binding="tns:SentimentServiceSoapBinding">
      <soap:address location="http://localhost:8080/BaseServices/services/Sentiment"/>
